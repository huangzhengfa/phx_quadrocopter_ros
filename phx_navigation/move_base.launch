<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find phx_navigation)/cfg/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find phx_navigation)/cfg/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find phx_navigation)/cfg/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find phx_navigation)/cfg/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find phx_navigation)/cfg/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
